{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "AnaFis",
  "version": "0.1.0",
  "identifier": "com.CokieMiner.AnaFis",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "AnaFis",
        "width": 800,
        "height": 600,
        "maximized": true,
        "decorations": false,
        "transparent": false,
        "center": true,
        "resizable": true,
        "maximizable": true,
        "minimizable": true,
        "closable": true,
        "skipTaskbar": false,
        "label": "main",
        "dragDropEnabled": false,
        "titleBarStyle": "Overlay"
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": [
      "deb",
      "rpm"
    ],
    "icon": [
      "icons/icon.png",
      "icons/icon.ico"
    ],
    "resources": [],
    "fileAssociations": [
      {
        "ext": [
          "anafispread"
        ],
        "name": "AnaFis Spreadsheet",
        "description": "AnaFis Spreadsheet File",
        "role": "Editor",
        "mimeType": "application/x-anafis-spreadsheet"
      }
    ],
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": null
    },
    "linux": {
      "deb": {},
      "rpm": {}
    }
  }
}